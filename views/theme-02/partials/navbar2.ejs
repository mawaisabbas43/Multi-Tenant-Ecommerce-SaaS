 <!-- header -->
    <header>
        <div class="container">
            <!-- top nav -->
            <nav class="top_nav d-flex pt-3 pb-1 ">
                <!-- logo -->
                
                <%if(store.logo.url!=null){%> 
                     <a href="/<%=storeId%>">
                     <img src="<%=store.logo.url%>" 
                        alt="logo" 
                        class="rounded"
                        width="60" height="60" >
                    </a>
                     
                  <%}else{%>
                     <h1><a class="navbar-brand"  href="/<%=storeId%>"><%=store.name%></a></h1>
                  <%}%>
                <div>
           
                </div>
                <!-- //logo -->
                
                <div class="w3ls_right_nav ml-auto d-flex">
                    
                <div class="nav-icon d-flex">
                       
                        <!-- shopping cart -->
                    <div class="cart-mainf">
                      <div class="hubcart hubcart2 cart cart bo">
                          <a class="btn top_hub_cart mt-1" href="/<%=storeId%>/cart" role="button" >
                             <i class="fas fa-shopping-cart">
                             <span> <%= cart.formattedTotals %> </span> (<span id="simpleCart_quantity" >
                                  <%= cart.items.length%> </span>)</i>
                              <a href="/<%=storeId%>/cart/empty/<%= nonce %>" >Empty cart</a>
                            </a>
                                
                      </div>
                    </div>
                        <!-- //shopping cart ends here -->
                  </div>
              </div>
                
            </nav>
            <!-- //top nav -->
            <!-- bottom nav -->
            <nav class="navbar navbar-expand-lg navbar-light justify-content-center">
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav mx-auto text-center">
                        <li class="nav-item nav-drop">
                            <div class="dropdown show">
                                <a <%if(pageName==="home"){%>id="active"<%}%> class="nav-drop" style="padding:0.51rem;" href="/<%=storeId%>" data-toggle="">
                                  Home
                                </a>
                              </div>
                        </li>
                         <li class="nav-item nav-drop">
                            <div class="dropdown show">
                                <a <%if(pageName==="categories"){%>id="active"<%}%> class="dropdown-toggle" style="padding:0.51rem;" href="#" id="dropdownMenuLink" data-toggle="dropdown">
                                  Categories
                                </a>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                  <nav class="vertical" style="width:100%">
                                                <ul>
                                                <%- include('./partials/treeView', {items: categories}) %>
                                                </ul>
                                              </nav>
                                </div>
                              </div>
                        </li>
                         <li class="nav-item nav-drop">
                            <div class="dropdown show">
                                <a <%if(pageName==="aboutUs"){%>id="active"<%}%> class="nav-drop " href="/<%=storeId%>/about-us" style="padding:0.51rem;" href="#" id="" data-toggle="">
                                  About Us
                                </a>
                              </div>
                        </li>
                         <li class="nav-item nav-drop">
                            <div class="dropdown show">
                                <a <%if(pageName==="contactUs"){%>id="active"<%}%> class="nav-drop " href="/<%=storeId%>/contact-us" style="padding:0.51rem;" href="#" id="" data-toggle="">
                                  Contact
                                </a>
                              </div>
                        </li>
                        <li class="nav-item nav-drop">
                            <div class="dropdown show">
                                <a <%if(pageName==="orders"){%>class="active"<%}%> class="nav-drop "href="/<%=storeId%>/orders" style="padding:0.51rem;" href="#" id="" data-toggle="">
                                  My Orders
                                </a>
                              </div>
                        </li>

                        
                       
                        
                    </ul>
                </div>
            </nav>
            <!-- //bottom nav -->
        </div>
        <!-- //header container -->
    </header>
    <!-- //header -->





