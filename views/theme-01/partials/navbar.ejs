 <div class="header-outs" id="home">
         <div class="header-bar">
            
            <div class="container-fluid">
               <div class="hedder-up row">
                  <div class="col-lg-3 col-md-3 logo-head">
                   <%if(store.logo.url!=null){%> 
                    <a href="/<%=storeId%>">
                     <img src="<%=store.logo.url%>" 
                        alt="logo" 
                        class="rounded"
                        width="60" height="60" >
                    </a>
                     
                  <%}else{%>
                     <h1><a class="navbar-brand"  href="/<%=storeId%>"><%=store.name%></a></h1>
                  <%}%>
                     
                  </div>
                  <div class="col-lg-5 col-md-6 search-right">
                    
                  </div>
                  <div class="col-lg-4 col-md-3 right-side-cart">
                     <div class="cart-icons">
                        <ul>
                          
                           <li class="toyscart toyscart2 cart cart box_1">
                              <div  class="last">
                                 
                                 <a class="top_toys_cart" href="/<%=storeId%>/cart">
                                 <i class="fas fa-cart-arrow-down">
                                 <span> <%= cart.formattedTotals %> </span> (<span id="simpleCart_quantity" >
                                  <%= cart.items.length%> </span>)</i>
                                  <a href="/<%=storeId%>/cart/empty/<%= nonce %>" >Empty cart</a>
                                 </a>
                              </div>
                           </li>
                        </ul>
                     </div>
                  </div>
               </div>
            </div>
            <nav class="navbar navbar-expand-lg navbar-light">
               <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
               <span class="navbar-toggler-icon"></span>
               </button>
               <div class="collapse navbar-collapse justify-content-center" id="navbarSupportedContent">
                  <ul class="navbar-nav ">
                     <li class="nav-item">
                        <a class="nav-link" href="/<%=storeId%>" <%if(pageName==="home"){%> class="active" <%}%> >Home</a>
                     </li>
                     <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Categories<b class="caret"></b>
                        </a>
                        <div class="dropdown-menu bg-coloor" aria-labelledby="navbarDropdown">
                           <nav class="vertical bg-coloor">
                              <ul>
                                 <%- include('./partials/treeView', {items: categories}) %>
                              </ul>                 
                           </nav>
                        </div>
                     </li>
                      <li class="nav-item">
                        <a class="nav-link"  href="/<%=storeId%>/about-us" <%if(pageName==="aboutUs"){%>class="active"<%}%> >About</a>
                     </li>
                     <li class="nav-item">
                        <a class="nav-link" href="/<%=storeId%>/contact-us" <%if(pageName==="contactUs"){%>class="active"<%}%> >Contact</a>
                     </li>
                     <li class="nav-item">
                        <a class="nav-link" href="/<%=storeId%>/orders" <%if(pageName==="orders"){%>class="active"<%}%> >My Orders</a>
                     </li>
                  </ul>
               </div>
            </nav>
         </div>
        
      </div>